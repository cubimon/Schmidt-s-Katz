	cp ${fontawesome_path}/step-forward.svg icons/fontawesome
	cp ${fontawesome_path}/fast-backward.svg icons/fontawesome
	cp ${fontawesome_path}/fast-forward.svg icons/fontawesome
	cp ${fontawesome_path}/volume-mute.svg icons/fontawesome
	cp ${fontawesome_path}/volume-down.svg icons/fontawesome
	cp ${fontawesome_path}/volume-up.svg icons/fontawesome
	cp ${fontawesome_path}/backward.svg icons/fontawesome
	cp ${fontawesome_path}/forward.svg icons/fontawesome
	cp ${fontawesome_path}/thumbs-up.svg icons/fontawesome
	cp ${fontawesome_path}/thumbs-down.svg icons/fontawesome
